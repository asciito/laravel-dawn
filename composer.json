{
    "name": "asciito/laravel-dawn",
    "description": "Laravel Dawn provides simple browser automation based on Laravel Dusk.",
    "keywords": ["laravel", "webdriver", "dawn", "browser", "automation"],
    "authors": [
        {
            "name": "Ayax CÃ³rdova",
            "email": "ayax.cordova@aydev.mx"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-zip": "*",
        "php-webdriver/webdriver": "^1.9.0",
        "nesbot/carbon": "^2.0",
        "illuminate/console": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "symfony/console": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/process": "^6.0",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^7.0|^8.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5.10|^10.0.1",
        "psy/psysh": "^0.11.12"
    },
    "suggest": {
        "ext-pcntl": "Used to gracefully terminate Dawn when tests are running."
    },
    "autoload": {
        "psr-4": {
            "Asciito\\LaravelDawn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asciito\\LaravelDawn\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asciito\\LaravelDawn\\DawnServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
